  "devDependencies": {
    "@types/node": "^14.14.10",
    "@types/prettier": "^2.1.5",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
